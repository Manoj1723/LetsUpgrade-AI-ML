     "text": [
      "square of 86 - 2 = 7394\n",
      "square of 18 - 2 = 322\n",
      "square of 36 - 2 = 1294\n",
      "square of 94 - 2 = 8834\n",
      "square of 14 - 2 = 194\n"
     ]
    }
   ],
   "source": [
